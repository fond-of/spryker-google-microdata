{
    "name": "fond-of-spryker/google-microdata",
    "description": "provides google structured product data feed (json)",
    "license": "MIT",
    "authors": [
        {
            "name": "Pascal Fischer",
            "email": "pascal.fischer@fondof.de"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.1",
        "spryker/money": "^1.0 || ^2.0",
        "spryker/kernel": "^3.43.0"
    },
    "require-dev": {
      "spryker/code-sniffer": "^0.14.7",
      "fond-of-codeception/spryker": "^1.0",
      "mikey179/vfsstream": "^1.6",
      "php-coveralls/php-coveralls": "^2.1",
      "sebastian/phpcpd": "^4.1"
    },
    "autoload": {
        "psr-4": {
            "FondOfSpryker\\": "src/FondOfSpryker/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FondOfSpryker\\": "tests/FondOfSpryker/",
            "Generated\\": "tests/Generated/"
        }
    }
}
